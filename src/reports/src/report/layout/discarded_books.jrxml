	<queryString>
		<![CDATA[SELECT
   CASE WHEN B.SUBTITLE IS NULL THEN B.TITLE
       ELSE B.TITLE || ' - ' || B.SUBTITLE
   END as TITLE,
   B.PURCHASE_DATE,
   B.AUTHOR,
   B.PUBLISHER,
   B.ISBN,
   B.EDITION,
   B.VOLUME,
   B.ISSUE_YEAR,
   B.NUM_PAGES,
   DB.DISCARD_DATE,
   DB.REASON
FROM BOOK B
JOIN DISCARDED_BOOK DB ON (DB.ID_BOOK = B.ID)
WHERE DB.DISCARD_DATE BETWEEN $P{DATE1} AND $P{DATE2}
ORDER BY B.TITLE, B.ID;]]>
	</queryString>
