	<parameter name="DATE1" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="DATE2" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    ID,
    TITLE,
    PURCHASE_DATE,
    AUTHOR,
    PUBLISHER,
    ISBN,
    EDITION,
    VOLUME,
    ISSUE_YEAR,
    NUM_PAGES,
    LOAN_DATE,
    APPLICANT,
    RETURN_DATE
FROM (
    SELECT
        B.ID,
        CASE WHEN B.SUBTITLE IS NULL THEN B.TITLE
       	ELSE B.TITLE || ' - ' || B.SUBTITLE
        END as TITLE,
        B.PURCHASE_DATE,
        B.AUTHOR,
        B.PUBLISHER,
        B.ISBN,
        B.EDITION,
        B.VOLUME,
        B.ISSUE_YEAR,
        B.NUM_PAGES,
        L.LOAN_DATE,
        L.APPLICANT,
        LI.RETURN_DATE
    FROM LOAN_ITEM LI
    JOIN LOAN L ON (L.ID = LI.ID_LOAN)
    JOIN BOOK B ON (B.ID = LI.ID_BOOK)
    WHERE (LI.RETURNED = TRUE) AND (L.LOAN_DATE BETWEEN $P{DATE1} AND $P{DATE2})
    UNION ALL
    SELECT
        B.ID,
        CASE WHEN B.SUBTITLE IS NULL THEN B.TITLE
       	ELSE B.TITLE || ' - ' || B.SUBTITLE
        END as TITLE,
        B.PURCHASE_DATE,
        B.AUTHOR,
        B.PUBLISHER,
        B.ISBN,
        B.EDITION,
        B.VOLUME,
        B.ISSUE_YEAR,
        B.NUM_PAGES,
        L.LOAN_DATE,
        L.APPLICANT,
        CAST('9999-12-31' AS DATE) AS RETURN_DATE
    FROM LOAN_ITEM LI
    JOIN LOAN L ON (L.ID = LI.ID_LOAN)
    JOIN BOOK B ON (B.ID = LI.ID_BOOK)
    WHERE (LI.RETURNED = FALSE) AND (L.LOAN_DATE BETWEEN $P{DATE1} AND $P{DATE2})
)
ORDER BY TITLE, ID, APPLICANT, PURCHASE_DATE, RETURN_DATE;]]>
	</queryString>
