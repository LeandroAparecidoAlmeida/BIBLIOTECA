	<queryString>
		<![CDATA[SELECT
   CASE WHEN SUBTITLE IS NULL THEN TITLE
       ELSE TITLE || ' - ' || SUBTITLE
   END as TITLE,
   PURCHASE_DATE,
   AUTHOR,
   PUBLISHER,
   ISBN,
   EDITION,
   VOLUME,
   ISSUE_YEAR,
   NUM_PAGES
FROM BOOK
WHERE ID NOT IN (
    SELECT
    	ID_BOOK
	FROM DISCARDED_BOOK
) AND PURCHASE_DATE BETWEEN $P{DATE1} AND $P{DATE2}
ORDER BY 1 ASC;]]>
	</queryString>
